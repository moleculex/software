<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="dao.Session" table="Session">
<id name="id" column="id" type="int">
	<generator class="assigned"></generator>
</id>
<property name="deveui" column="deveui" type="binary"></property>
<property name="devAddr" column="devAddr" type="binary"></property>
<property name="devNonce" column="devNonce" type="binary"></property>
<property name="appNonce" column="appNonce" type="binary"></property>
<property name="gweui" column="gweui" type="binary"></property>
</class>
</hibernate-mapping>
